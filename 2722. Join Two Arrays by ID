/**
 * @param {Array} arr1
 * @param {Array} arr2
 * @return {Array}
 */
var join = function(arr1, arr2) {
    let item =arr1.concat(arr2);
    let ans={};
    for(const i of item){
        if(!ans[i.id]){
            ans[i.id]=i;
            continue;
        }
        ans[i.id]={...ans[i.id],...i};
    }
    return Object.values(ans);
};
